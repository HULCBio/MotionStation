# Script to compute rotating parts center&translation
# according to the cable length
DEF Cable_Length_Control Script {
	eventIn	 SFVec3f set_CableLength
	eventOut SFVec3f LoadTransl_changed
	field SFFloat length 14.1
	field SFNode Node1 USE Parts_Rot_ZAxis
	field SFNode Node2 USE Parts_Rot_XAxis
	field SFNode Node3 USE Load

	url	"vrmlscript:
	function set_CableLength (value) {
    	length = value [1];
		Node1.set_center = new SFVec3f (0, 0.5 * length, 0 );
		Node1.set_translation =  new SFVec3f (0, -0.5 * length, 0 );
		Node2.set_center = new SFVec3f (0, 0.5 * length, 0 );
		Node3.set_translation = new SFVec3f (0, -0.5 * length, 0 );
	}
	"
}

# Script to compute SetPoint tracers translations
DEF SetPoint_Tracers_Control Script {
	eventIn	 SFVec3f set_SP_Ttransl
	eventOut SFVec3f SP_Transl_changed
	field SFNode Node1 USE SP_Tracer_X
	field SFNode Node2 USE SP_Tracer_Y
	field SFNode Node3 USE SP_Tracer_Z

	url	"vrmlscript:
	function set_SP_Ttransl (value) {
		Node1.set_translation = new SFVec3f (10, 0.05, value[2]);
		Node2.set_translation =  new SFVec3f (value[0], 5, value[2]);
		Node3.set_translation = new SFVec3f (value[0], 0.05, 10);
	}
	"
}

# Script to move Shadow load to the new position
# according to the crane take-off and drop-off.
# (Shadow invisible during move, reappears after Load release.)
DEF Move_Shadow Script {
	eventIn	SFInt32 Load_OnHook
	field SFInt32 State 0
	field SFVec3f LastShadowPos 0 0 0
	field SFNode LS USE Load_Shadow
	field SFNode CX USE Crane_X
	field SFNode CZ USE Crane_Z

	url	"vrmlscript:
	function Load_OnHook (value) {

		if (State==0)
		{ 
			if (value==1) {
				// If we wanted to remember last Load position and use it afterwards
				// LastShadowPos = LS.translation;
				State = 1;
			}
		}
		else 
		{
		   	if (value==0) {
				CurCX = CX.translation;
				CurCZ = CZ.translation;
				// Drop the Shadow always on the Ground (y=0), right under the cat
				LS.translation = new SFVec3f (CurCX[0], 0, CurCZ[2]);		
				State = 0;
			}
		}
		
	}
	"
}

# Button Scripts 
DEF PB_XMinus_Script Script {
	eventIn	SFBool PButton_Active
	field SFBool state FALSE
	field SFNode Node1 USE PB_XMinus_Mat
	field SFNode Node2 USE PB_XMinus_Core

	url	"vrmlscript:
	function PButton_Active (value) {

		if (state)
		{ 
			Node1.diffuseColor = new SFColor (0, 0.4, 0);
			Node2.translation = new SFVec3f (0, 1, 0);
			state = FALSE;
		}
		else 
		{ 
			Node1.diffuseColor = new SFColor (0.4, 0, 0);
			Node2.translation = new SFVec3f (0, 0, 0);		
			state = TRUE;
		}
		
	}
	"
}

