# $Revision: 1.5 $
Library {
  Name			  "rtwextraslib"
  Version		  2.20
  BlockDefaults {
    Orientation		    right
    ForegroundColor	    black
    BackgroundColor	    white
    DropShadow		    off
    NamePlacement	    normal
    FontName		    "Helvetica"
    FontSize		    10
    FontWeight		    normal
    FontAngle		    normal
    ShowName		    on
  }
  AnnotationDefaults {
    HorizontalAlignment	    center
    VerticalAlignment	    middle
    ForegroundColor	    black
    BackgroundColor	    white
    DropShadow		    off
    FontName		    "Helvetica"
    FontSize		    10
    FontWeight		    normal
    FontAngle		    normal
  }
  LineDefaults {
    FontName		    "Helvetica"
    FontSize		    9
    FontWeight		    normal
    FontAngle		    normal
  }
  System {
    Name		    "rtwextraslib"
    Location		    [166, 420, 525, 536]
    Open		    on
    ToolBar		    off
    StatusBar		    off
    ScreenColor		    white
    PaperOrientation	    landscape
    PaperPositionMode	    auto
    PaperType		    usletter
    PaperUnits		    inches
    Block {
      BlockType		      SubSystem
      Name		      "Subsystem"
      Ports		      [0, 0, 0, 0, 0]
      Position		      [26, 20, 99, 81]
      BackgroundColor	      green
      ShowName		      off
      FontName		      "Arial"
      FontSize		      12
      FontWeight	      bold
      ShowPortLabels	      on
      MaskDisplay	      "disp('RTW\\nExtras')"
      MaskIconFrame	      on
      MaskIconOpaque	      on
      MaskIconRotate	      none
      MaskIconUnits	      autoscale
      System {
	Name			"Subsystem"
	Location		[246, 293, 607, 391]
	Open			off
	ToolBar			off
	StatusBar		off
	ScreenColor		white
	PaperOrientation	landscape
	PaperPositionMode	auto
	PaperType		usletter
	PaperUnits		inches
	Block {
	  BlockType		  SubSystem
	  Name			  "Subsystem"
	  Ports			  [0, 0, 0, 0, 0]
	  Position		  [25, 20, 98, 73]
	  BackgroundColor	  yellow
	  ShowName		  off
	  List {
	    ListType		    RTWdata
	    FcnVisibility	    "1"
	    FcnName		    "2"
	    TLCFile		    "fcncallconfig"
	  }
	  ShowPortLabels	  on
	  MaskType		  "Function-call Configuration"
	  MaskDescription	  "This block controls the name and visibility"
" of the generated function-call subsystem.  By default, RTW mangles the name "
"of the function (to guarantee uniqueness) and declares the function global (s"
"o that others may call it).\n\nThis behavior is configurable with the functio"
"n name and function visibility, respectively.\n"
	  MaskPromptString	  "Function Name:|Function Visibility:"
	  MaskStyleString	  "popup(System Block Name|Don't Care (mangled"
")),popup(Public|Private (local to file))"
	  MaskTunableValueString  "on,on"
	  MaskVariables		  "fcnName=@1;fcnVisibility=@2;"
	  MaskInitialization	  "data.FcnVisibility=int2str(fcnVisibility);"
"\ndata.FcnName=int2str(fcnName);\ndata.TLCFile='fcncallconfig';\nset_param(gc"
"b, 'rtwdata',data);"
	  MaskDisplay		  "disp('Function-call\\nConfiguration\\n(RTW)"
"')"
	  MaskIconFrame		  on
	  MaskIconOpaque	  on
	  MaskIconRotate	  none
	  MaskIconUnits		  autoscale
	  MaskValueString	  "Don't Care (mangled)|Public"
	  System {
	    Name		    "Subsystem"
	    Location		    [203, 331, 569, 538]
	    Open		    off
	    ToolBar		    off
	    StatusBar		    off
	    ScreenColor		    white
	    PaperOrientation	    landscape
	    PaperPositionMode	    auto
	    PaperType		    usletter
	    PaperUnits		    inches
	  }
	}
      }
    }
  }
}
