<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>13</matlabrelease>
<name>Real-Time Windows Target</name>
<type>simulink</type>
<icon>$toolbox/matlab/icons/boardicon.gif</icon>
<help_location>$docroot/toolbox/rtwin</help_location>

<list>

<listitem>
<label>Block Library</label>
<callback>rtwinlib</callback>
<icon>$toolbox/simulink/simulink/blockdiagramicon.gif</icon>
</listitem>

<listitem>
<label>Install real-time kernel</label>
<callback>rtwintgt -setup</callback>
<icon>$toolbox/matlab/icons/unknownicon.gif</icon>
</listitem>

<listitem>
<label>Uninstall real-time kernel</label>
<callback>rtwintgt -uninstall</callback>
<icon>$toolbox/simulink/simulink/delete.gif</icon>
</listitem>

<listitem>
<label>Help</label>
<callback>doc rtwin/</callback>
<icon>$toolbox/matlab/icons/book_sim.gif</icon>
</listitem>

<listitem>
<label>Demos</label>
<callback>demo('Simulink','Real-Time Windows Target')</callback>
<icon>$toolbox/matlab/icons/demoicon.gif</icon>
</listitem>

<listitem>
<label>Product Page (Web)</label>
<callback>web http://www.mathworks.com/products/rtwt/ -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

</list>

</productinfo>
