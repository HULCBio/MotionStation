machine
table 0
SF_TEXT `Machine` bold underline 0 0 20  	
machine.id SF_EMPTY important underline rightJustify 1 0 80  		^[sf('get', ^, '.name'), ' (', sf('get', ^, '.rgTag'), ')' ]

machine.description SF_EMPTY important 1 1 80 		@tableCellEmpty( 1, 1 )
SF_TEXT `Description` bold 0 1 20  		@tableCellEmpty( 1, 1 )

machine.created SF_EMPTY 1 2 80 			@tableCellEmpty( 1, 2 )
SF_TEXT `Created` bold 0 2 20 			@tableCellEmpty( 1, 2 )

machine.creator SF_EMPTY 1 3 80		@tableCellEmpty( 1, 3 )
SF_TEXT `Creator` bold 0 3 20			@tableCellEmpty( 1, 3 )

machine.version SF_EMPTY 1 4 80		@tableCellEmpty( 1, 4 )
SF_TEXT `Version` bold 0 4 20			@tableCellEmpty( 1, 4 )

~machine.charts SF_EMPTY important 1 5 80		^sf('FullNameOf', ^, '/' )		@tableCellEmpty( 1, 5 )
SF_TEXT `Charts` bold 0 5 20															@tableCellEmpty( 1, 5 )

~machine.sfLinks SF_EMPTY important 1 6 80	$rg_handle_link_block('set', $)	^get_full_path_of_block( rg_handle_link_block('get', ^) )  @tableCellEmpty( 1, 6 )
SF_TEXT `Library Links` bold 0 6 20					@tableCellEmpty( 1, 6 )

~machine.id SF_EMPTY important 1 7 80      $sf('DataOf', $) ^sf('get', ^, '.name')		@tableCellEmpty( 1, 7 )
SF_TEXT `Data` bold 0 7 20  																				@tableCellEmpty( 1, 7 )

~machine.id SF_EMPTY important 1 8 80       $sf('EventsOf', $) ^sf('get', ^, '.name')		@tableCellEmpty( 1, 8 )
SF_TEXT `Events` bold 0 8 20  																				@tableCellEmpty( 1, 8 )	
end table 1

define
sharePage 1
end define

end machine



