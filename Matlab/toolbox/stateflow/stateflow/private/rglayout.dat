end table 2

table 3
data.id SF_EMPTY important  1 0 35 @tableCellEmpty( 1, 0 ) | (sf ('get', @, '.parsedInfo.range.minimum') == -inf & sf ('get', @, '.parsedInfo.range.maximum') == inf) ^[sf('get',^,'.props.range.minimum') ' ... ' sf('get',^,'.props.range.maximum') ]
SF_TEXT `Range` bold  0 0 15  	@tableCellEmpty( 1, 0 )

data.id SF_EMPTY 3 0 35  @sf('get', @,'.parsedInfo.initialValue') == 0 	^sf( 'get',^,'.props.initialValue')
SF_TEXT `Init Val` bold  2 0 15	@tableCellEmpty( 3, 0 )


end table 3

table 4

data.id SF_EMPTY important  1 0 35 @tableCellEmpty( 1, 0 ) | length(sf('get',@,'.parsedInfo.array.size')) == 0   ^sf('get',^,'.props.array.size')
