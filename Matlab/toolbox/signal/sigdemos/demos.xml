<demos>
   <name>Signal Processing</name>
   <type>toolbox</type>
   <icon>$toolbox/matlab/icons/matlabicon.gif</icon>
   <description source="file">html/index.html</description>
   
   <demosection>
      <label>Filter Design and Analysis</label>
      <demoitem>
         <label>Introduction to the Filter Design and Analysis Tool (FDATool)</label>
         <callback>web([matlabroot,filesep,'toolbox',filesep,'signal',filesep,'sigdemos',filesep,'viewlet',filesep,'FDATool_tutorial_viewlet.html'],'-browser');</callback>
      </demoitem>
      <demoitem>
         <label>Interactive Lowpass Filter Design</label>
         <callback>filtdemo</callback>
      </demoitem>
      <demoitem>
         <label>Getting Started with Discrete-Time Filter (DFILT) Objects</label>
         <file>html/dfiltdemo.html</file>
      </demoitem>
      <demoitem>
         <label>Filter Analysis using FVTool and its Application Program Interface (API)</label>
         <file>html/fvtooldemo.html</file>
      </demoitem>
      <demoitem>
         <label>Interactive Analysis of Second-Order Sections Filters</label>
         <callback>sosdemo</callback>
      </demoitem>
   </demosection>

   <demosection>
      <label>Data Filtering</label>
      <demoitem>
         <label>Advanced Filtering with Discrete-Time Filter (DFILT) Objects</label>
         <file>html/dfiltfilteringdemo.html</file>
      </demoitem>
      <demoitem>
         <label>Denoising ECG Signals with Savitzky-Golay Filters</label>
         <callback>sgolaydemo</callback>
      </demoitem>
      <demoitem>
         <label>Generating Guitar Chords using the Karplus-Strong Algorithm</label>
         <file>html/filterguitardemo.html</file>
         <callback>playshow filterguitardemo</callback>
      </demoitem>
      <demoitem>
         <label>Interactive Discrete-Time Filter Guitar</label>
         <callback>filterguitar</callback>
      </demoitem>
   </demosection>

   <demosection>
      <label>Spectral Analysis and Statistical Signal Processing</label>
      <demoitem>
         <label>Getting Started with Spectral Analysis Objects</label>
         <file>html/spectralanalysisobjsdemo.html</file>
      </demoitem>
      <demoitem>
         <label>Measuring the Power of Deterministic Periodic Signals</label>
         <file>html/deterministicsignalpower.html</file>
      </demoitem>
      <demoitem>
         <label>Power Spectral Density Demo</label>
         <callback>psddemo</callback>
      </demoitem>
      <demoitem>
         <label>Spectrogram Demo</label>
         <callback>specgramdemo</callback>
      </demoitem>
      <demoitem>
         <label>Linear Prediction and Autoregressive Modeling</label>
         <file>html/lpc-ardemo.html</file>
         <callback>playshow lpc-ardemo</callback>
      </demoitem>
   </demosection>

   <demosection>
      <label>Transforms</label>
      <demoitem>
         <label>Chirp-z Transform</label>
         <callback>cztdemo</callback>
      </demoitem>
      <demoitem>
         <label>Discrete Fourier Transform</label>
         <callback>sigdemo1</callback>
      </demoitem>
      <demoitem>
         <label>Dual-Tone Multi-Frequency (DTMF) Signal Detection</label>
         <file>html/dtmfdemo.html</file> 
         <callback>dtmfdemo</callback>
      </demoitem>
   </demosection>

   <demosection>
      <label>Miscellaneaous</label>
      <demoitem>
         <label>Waveform Generation</label>
         <file>html/waveformdemo.html</file> 
         <callback>playshow waveformdemo</callback>
     </demoitem>
      <demoitem>
         <label>Modulation/Demodulation</label>
         <callback>moddemo</callback>
      </demoitem>
      <demoitem>
         <label>Vector Transition Processing using Windows</label>
         <callback>windtrandemo</callback>
      </demoitem>
   </demosection>
</demos>
