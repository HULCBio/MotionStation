<demos>
   <name>Simulink Report Generator</name>
   <type>simulink</type>
   <icon>$toolbox/matlab/icons/simulinkicon.gif</icon>
   <description isCdata="no">
<p>The Simulink Report Generator provides a set of flexible,
customizable components which document Simulink and Stateflow
projects.</p>
   </description>
   <demoitem>
      <label>Report Generator Block Library</label>
      <callback>rptgenlib</callback>
   </demoitem>
   <demoitem>
      <label>Simulink Default Report</label>
      <callback>setedit simulink-default.rpt</callback>
   </demoitem>
   <demoitem>
      <label>Simulink Framework Report</label>
      <callback>setedit simulink-blank.rpt</callback>
   </demoitem>
   <demoitem>
      <label>Simulink Summary Report (req RTW)</label>
      <callback>setedit simulink-summary.rpt</callback>
   </demoitem>
   <demoitem>
      <label>Simulink Images Report</label>
      <callback>setedit simulink-images.rpt</callback>
   </demoitem>
   <demoitem>
      <label>Simulink Dynamic Report</label>
      <callback>setedit simulink-dynamic.rpt</callback>
   </demoitem>
   <demoitem>
      <label>Stateflow Default Report</label>
      <callback>setedit stateflow-default.rpt</callback>
      <dependency>Stateflow</dependency>
   </demoitem>
</demos>
