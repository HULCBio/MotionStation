<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>


<matlabrelease>13</matlabrelease>
<name>MATLAB</name>
<type>matlab</type>
<icon>$toolbox/matlab/icons/matlabicon.gif</icon>
<help_location>$docroot/techdoc</help_location>

<list>

<listitem>
<label>Import Wizard</label>
<callback>uiimport</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>Profiler</label>
<callback>profile viewer</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>GUIDE (GUI Builder)</label>
<callback>guide</callback>
<icon>$toolbox/matlab/icons/guideicon.gif</icon>
</listitem>

<listitem>
<label>Notebook</label>
<callback>notebook</callback>
<unsupported-platform>unix</unsupported-platform>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>Plot Tools</label>
<callback>figure; plottools</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>Help</label>
<callback>doc matlab/</callback>
<icon>$toolbox/matlab/icons/book_mat.gif</icon>
</listitem>

<listitem>
<label>Demos</label>
<callback>demo matlab</callback>
<icon>$toolbox/matlab/icons/demoicon.gif</icon>
</listitem>

<listitem>
<label>MATLAB Central (Web)</label>
<callback>web http://www.mathworks.com/matlabcentral_redirect -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

<listitem>
<label>Product Page (Web)</label>
<callback>web http://www.mathworks.com/products/matlab/ -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

</list>

</productinfo>
