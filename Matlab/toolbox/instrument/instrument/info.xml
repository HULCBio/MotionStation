<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>13</matlabrelease> 
<name>Instrument Control</name> 
<type>toolbox</type> 
<icon>$toolbox/matlab/icons/matlabicon.gif</icon> 
<help_location>$docroot/toolbox/instrument</help_location>

<preference_panel>
<label>Instrument Control</label>
<source>com.mathworks.toolbox.instrument.util.PreferencePanel</source>
</preference_panel>

<list>

<listitem>
<label>Test and Measurement Tool (tmtool)</label>
<callback>tmtool</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>MATLAB Instrument Driver Editor (midedit)</label>
<callback>midedit</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>MATLAB Instrument Driver Testing Tool (midtest)</label>
<callback>midtest</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>ASCII Communication Tool (instrcomm)</label>
<callback>instrcomm</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>Instrument Creation Tool (instrcreate)</label>
<callback>instrcreate</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem> 
<label>Help</label> 
<callback>doc instrument/;</callback> 
<icon>$toolbox/matlab/icons/book_mat.gif</icon> 
</listitem> 

<listitem> 
<label>Demos</label> 
<callback>demo toolbox Instrument</callback> 
<icon>$toolbox/matlab/icons/demoicon.gif</icon> 
</listitem> 

<listitem> 
<label>Product Page (Web)</label> 
<callback>web http://www.mathworks.com/products/instrument/ -browser;</callback> 
<icon>$toolbox/matlab/icons/webicon.gif</icon> 
</listitem> 

</list>

</productinfo>
