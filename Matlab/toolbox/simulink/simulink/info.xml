<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>13</matlabrelease>
<name>Simulink</name>
<type>simulink</type>
<icon>$toolbox/simulink/simulink/simulinkicon.gif</icon>
<help_location>$docroot/toolbox/simulink</help_location>
<help_addon name="SB2SL" productlevel="false">$docroot/toolbox/sb2sl</help_addon>

<preference_panel>                                                         
<label>Simulink</label>                                                    
<source>com.mathworks.toolbox.simulink.preferences.EditingConfig</source>  
    <preference_panel>                                                    
    <label>Fonts</label>                                                  
    <source>com.mathworks.toolbox.simulink.preferences.FontConfig</source>
    </preference_panel>                                                   
    <preference_panel>                                                    
    <label>Simulation</label>                                                                                                                        
	<source>com.mathworks.toolbox.simulink.preferences.ExecutionConfig</source>
    </preference_panel>                                                   
</preference_panel>

<list>

<listitem>
<label>Library Browser</label>
<callback>simulink</callback>
<icon>$toolbox/simulink/simulink/simulinkicon.gif</icon>
</listitem>

<listitem>
<label>Help</label>
<callback>doc simulink/</callback>
<icon>$toolbox/matlab/icons/book_sim.gif</icon>
</listitem>

<listitem>
<label>Demos</label>
<callback>demo simulink</callback>
<icon>$toolbox/matlab/icons/demoicon.gif</icon>
</listitem>

<listitem>
<label>Product Page (Web)</label>
<callback>web http://www.mathworks.com/products/simulink/ -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

</list>

</productinfo>
