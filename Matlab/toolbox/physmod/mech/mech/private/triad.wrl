Shape {
	geometry	IndexedLineSet {
		color	Color {
			color [1 0 0,
				   0 1 0,
				   0 0 1]
		}

		coord	Coordinate {
			point	[ 0 0 0,
					  1 0 0,
					  0 1 0,
					  0 0 1 ]
		}
		colorPerVertex FALSE

		coordIndex	[ 0, 1, -1, 0, 2, -1, 0, 3, -1]
		colorIndex	[0, 1, 2, 3]
	}

}
