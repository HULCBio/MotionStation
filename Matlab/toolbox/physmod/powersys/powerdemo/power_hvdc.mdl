	Annotation {
	  Name			  "This demonstration  illustrates steady-stat"
"e and transient performance\nof a simple 500 MW (250 kV-2kA) HVDC transmissio"
"n system"
	  Position		  [34, 22]
	  HorizontalAlignment	  "left"
	  VerticalAlignment	  "top"
	  FontSize		  14
