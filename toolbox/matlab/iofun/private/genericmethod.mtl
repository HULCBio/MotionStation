function $OUTPUT$$METHODNAME$$INPUT$
%$METHODNAME$$INPUT$

% Build up the argument lists.
$PARAMETERDEFINITION$

% Create the message and make the call.
soapMessage = createSoapMessage( ...
    '$TARGETNAMESPACEURI$', ...
    '$METHODNAME$', ...
    values,names,types);
response = callSoapService( ...
    obj.endpoint, ...
    '$SOAPACTION$', ...
    soapMessage);

% Convert to MATLAB data types.
results = parseSoapResponse(response);
$OUTPUT$deal(results{:});
