#VRML V2.0 utf8

DEF	vehicleCOG Transform {
	children [
		DEF chassisCOG Transform {
			children Inline {
				url	"chassisCOG.wrl"
			}
		}
		DEF chairCOGL Transform {	   
		# left and right, back and forth, up and down
			translation	-160 -180 -170
			rotation 1 0 0 0.22
			scale	1.8 1.8 1.8
			children Inline {
				url	"chairCOG.wrl"
			}
		}

		DEF chairCOGR Transform {	   
		# left and right, back and forth, up and down
			translation	160 -180 -170
			rotation 1 0 0 0.22
			scale	1.8 1.8 1.8
			children Inline {
				url	"chairCOG.wrl"
			}
		}
	]
}
