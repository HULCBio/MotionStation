rgbsxn1920\headery0\footery0\pgndec\pgnrestart\titlepg{\headerf\pard\sl-240\sb770\sa430\plain\tqc\tx4680\tqr\tx9360 {}\tab {}\tab {}\par}{\footerf\pard\sl-240\sb770\sa910\plain\tqc\tx4680\tqr\tx9360 {}\tab {}\tab {\i\fs20 \chpgn }\par}{\headerl\pard\sl-240\sb770\sa430\plain\tqc\tx4680\tqr\tx9360 {}\tab {}\tab {\i\fs20 Chapter 1. Peaks Code}\par}{\footerl\pard\sl-240\sb770\sa910\plain\tqc\tx4680\tqr\tx9360 {}\tab {}\tab {\i\fs20 \chpgn }\par}{\headerr\pard\sl-240\sb770\sa430\plain\tqc\tx4680\tqr\tx9360 {}\tab {}\tab {\i\fs20 Chapter 1. Peaks Code}\par}{\footerr\pard\sl-240\sb770\sa910\plain\tqc\tx4680\tqr\tx9360 {}\tab {}\tab {\i\fs20 \chpgn }\par}\pard\sb311\s1\sl539 {\*\bkmkstart _12}{\*\bkmkend _12}\b\fs41\f1 Chapter 1. Peaks Code \keepn\hyphpar0\par\pard\sb207\li960\sl234 \b0\fs18\f2 function  [xz,y,z] = peaks(arg1,arg2);\sa0\par\fi0\sb0
%PEAKS  A sample function of two variables.\sa0\par\fi0\sb0
%   PEAKS is a function of two variables, obtained by translating and\sa0\par\fi0\sb0
%   scaling Gaussian distributions, which is useful for demonstrating\sa0\par\fi0\sb0
%   MESH, SURF, PCOLOR, CONTOUR, etc.\sa0\par\fi0\sb0
%   There are several variants of the calling sequence:\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%       Z = PEAKS;\sa0\par\fi0\sb0
%       Z = PEAKS(N);\sa0\par\fi0\sb0
%       Z = PEAKS(V);\sa0\par\fi0\sb0
%       Z = PEAKS(X,Y);\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%       PEAKS;\sa0\par\fi0\sb0
%       PEAKS(N);\sa0\par\fi0\sb0
%       PEAKS(V);\sa0\par\fi0\sb0
%       PEAKS(X,Y);\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%       [X,Y,Z] = PEAKS;\sa0\par\fi0\sb0
%       [X,Y,Z] = PEAKS(N);\sa0\par\fi0\sb0
%       [X,Y,Z] = PEAKS(V);\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%   The first variant produces a 49-by-49 matrix.\sa0\par\fi0\sb0
%   The second variant produces an N-by-N matrix.\sa0\par\fi0\sb0
%   The third variant produces an N-by-N matrix where N = length(V).\sa0\par\fi0\sb0
%   The fourth variant evaluates the function at the given X and Y,\sa0\par\fi0\sb0
%   which must be the same size.  The resulting Z is also that size.\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%   The next four variants, with no output arguments, do a SURF\sa0\par\fi0\sb0
%   plot of the result.\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%   The last three variants also produce two matrices, X and Y, for\sa0\par\fi0\sb0
%   use in commands such as PCOLOR(X,Y,Z) or SURF(X,Y,Z,DEL2(Z)).\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%   If not given as input, the underlying matrices X and Y are\sa0\par\fi0\sb0
%       [X,Y] = MESHGRID(V,V) \sa0\par\fi0\sb0
%   where V is a given vector, or V is a vector of length N with\sa0\par\fi0\sb0
%   elements equally spaced from -3 to 3.  If no input argument is\sa0\par\fi0\sb0
%   given, the default N is 49.\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
%   CBM, 2-1-92, 8-11-92, 4-30-94.\sa0\par\fi0\sb0
%   Copyright 1984-2000 The MathWorks, Inc. \sa0\par\fi0\sb0
%   $Revision: 5.8 $  $Date: 2000/06/01 03:46:45 $\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
if nargin == 0\sa0\par\fi0\sb0
    dx = 1/8;\sa0\par\fi0\sb0
    [x,y] = meshgrid(-3:dx:3);\sa0\par\fi0\sb0
elseif nargin == 1\sa0\par\fi0\sb0
    if length(arg1) == 1\sa0\par\fi0\sb0
        [x,y] = meshgrid(-3:6/(arg1-1):3);\sa0\par\fi0\sb0
    else\sa0\par\fi0\sb0
        [x,y] = meshgrid(arg1,arg1);     \sa0\par\fi0\sb0
    end\sa0\par\fi0\sb0
else\sa0\par\fi0\sb0
    x = arg1; y = arg2;\sa0\par\fi0\sb0
end\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
z =  3*(1-x).^2.*exp(-(x.^2) - (y+1).^2) ...\sa0\par\fi0\sb0
   - 10*(x/5 - x.^3 - y.^5).*exp(-x.^2-y.^2) ...\sa0\par\fi0\sb0
   - 1/3*exp(-(x+1).^2 - y.^2);\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
if nargout > 1\sa0\par\fi0\sb0
    xz = x;\sa0\par\fi0\sb0
elseif nargout == 1\sa0\par\fi0\sb0
    xz = z;\sa0\par\fi0\sb0
else\sa0\par\fi0\sb0
    % Self demonstration\sa0\par\fi0\sb0
    disp(' ')\sa0\par\fi0\sb0
    disp('z =  3*(1-x).^2.*exp(-(x.^2) - (y+1).^2) ... ')\sa0\par\fi0\sb0
    disp('   - 10*(x/5 - x.^3 - y.^5).*exp(-x.^2-y.^2) ... ')\sa0\par\fi0\sb0
    disp('   - 1/3*exp(-(x+1).^2 - y.^2) ')\sa0\par\fi0\sb0
    disp(' ')\sa0\par\fi0\sb0
    surf(x,y,z)\sa0\par\fi0\sb0
    axis([min(min(x)) max(max(x)) min(min(y)) max(max(y)) ...\sa0\par\fi0\sb0
          min(min(z)) max(max(z))])\sa0\par\fi0\sb0
    xlabel('x'), ylabel('y'), title('Peaks')\sa0\par\fi0\sb0
end\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
