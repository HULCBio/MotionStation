%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%\sa0\par\fi0\sb0
function LocalMakeApplication(h,btnString)\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
figWd=200;\sa0\par\fi0\sb0
figHt=200;\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
btn=uicontrol('Parent',h,...\sa0\par\fi0\sb0
   'String',btnString,...\sa0\par\fi0\sb0
   'FontSize',18,...\sa0\par\fi0\sb0
   'Units','points');\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
btnExtent=get(btn,'Extent');\sa0\par\fi0\sb0
btnHt=btnExtent(4)+10;\sa0\par\fi0\sb0
btnWd=btnExtent(3)+10;\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
cbString=sprintf('set(gcbo,''Position'',[rand(1)*%0.2g rand(1)*%0.2g %0.2g %0.2g])',...\sa0\par\fi0\sb0
   figWd-btnWd-10,figHt-btnHt-10,btnWd,btnHt);\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
set(btn,...\sa0\par\fi0\sb0
   'Position',[10 10 btnWd btnHt],...\sa0\par\fi0\sb0
   'Callback',cbString);\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
