%FIGLOOPFIGURES creates figures for figloop-tutorial.rpt\sa0\par\fi0\sb0
%   FIGLOOPFIGURES creates five figures which are used by\sa0\par\fi0\sb0
%   the Report Generator setup file "figloop-tutorial.rpt".\sa0\par\fi0\sb0
%   To run this tutorial, type "setedit figloop-tutorial"\sa0\par\fi0\sb0
%   at the command prompt.\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%   Figure 1: Membrane Data\sa0\par\fi0\sb0
%   Figure 2: Invisible Membrane Data\sa0\par\fi0\sb0
%   Figure 3: An Application\sa0\par\fi0\sb0
%   Figure 4: An Invisible Application\sa0\par\fi0\sb0
%   Figure 5: Peaks Data\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%   Figures 2 and 4 are invisible.\sa0\par\fi0\sb0
%   Figures 3 and 4 have HandleVisibility='off'\sa0\par\fi0\sb0
%   Figure  5 is the current figure\sa0\par\fi0\sb0
%\sa0\par\fi0\sb0
%   FIGLOOPFIGURES deletes any existing figures which have\sa0\par\fi0\sb0
%   tag 'peaks' 'app' or 'membrane'\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
%   Copyright (c) 1997-2000 by The MathWorks, Inc.\sa0\par\fi0\sb0
%   $Revision: 1.3 $  $Date: 2000/04/28 20:33:02 $\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
%delete existing figures with used tag names\sa0\par\fi0\sb0
allFigs=allchild(0);\sa0\par\fi0\sb0
delete([findall(allFigs,'flat','tag','membrane');...\sa0\par\fi0\sb0
   findall(allFigs,'flat','tag','peaks');...\sa0\par\fi0\sb0
   findall(allFigs,'flat','tag','app')]);\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
figName=\{'Membrane Data'\sa0\par\fi0\sb0
   'Invisible Membrane Data'\sa0\par\fi0\sb0
   'An Application'\sa0\par\fi0\sb0
   'An Invisible Application'\sa0\par\fi0\sb0
   'Peaks Data'\};\sa0\par\fi0\sb0
figTag=\{'membrane'\sa0\par\fi0\sb0
   'membrane'\sa0\par\fi0\sb0
   'app'\sa0\par\fi0\sb0
   'app'\sa0\par\fi0\sb0
   'peaks'\};\sa0\par\fi0\sb0
figVisible=\{'on'\sa0\par\fi0\sb0
   'off'\sa0\par\fi0\sb0
   'on'\sa0\par\fi0\sb0
   'off'\sa0\par\fi0\sb0
   'on'\};\sa0\par\fi0\sb0
figHandleVisible=\{'on'\sa0\par\fi0\sb0
   'on'\sa0\par\fi0\sb0
   'off'\sa0\par\fi0\sb0
   'off'\sa0\par\fi0\sb0
   'on'\};\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
for i=1:length(figName)\sa0\par\fi0\sb0
   hList(i)=figure('Name',figName\{i\},...\sa0\par\fi0\sb0
      'Tag',figTag\{i\},...\sa0\par\fi0\sb0
      'HandleVisibility',figHandleVisible\{i\},...\sa0\par\fi0\sb0
      'Visible','off',...\sa0\par\fi0\sb0
      'Units','points',...\sa0\par\fi0\sb0
      'Position',[90+30*i 230-30*i 200 200]);\sa0\par\fi0\sb0
end\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
LocalMakeMembrane(hList(1));\sa0\par\fi0\sb0
LocalMakeMembrane(hList(2));\sa0\par\fi0\sb0
LocalMakeApplication(hList(3),'Push Me');\sa0\par\fi0\sb0
LocalMakeApplication(hList(4),'Invisible');\sa0\par\fi0\sb0
LocalMakePeaks(hList(5));\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
set(hList,\{'Visible'\},figVisible);\sa0\par\fi0\sb0
set(0,'CurrentFigure',hList(5));\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
drawnow\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
