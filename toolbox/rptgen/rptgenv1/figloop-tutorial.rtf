function LocalMakeMembrane(h)\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
ax=axes('Parent',h,...\sa0\par\fi0\sb0
   'Xlim',[0 31],...\sa0\par\fi0\sb0
   'Ylim',[0 31],...\sa0\par\fi0\sb0
   'Xgrid','on',...\sa0\par\fi0\sb0
   'Ygrid','on',...\sa0\par\fi0\sb0
   'Zgrid','on',...\sa0\par\fi0\sb0
   'View',[-37.5 35]);\sa0\par\fi0\sb0
surface(membrane,'Parent',ax);\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
