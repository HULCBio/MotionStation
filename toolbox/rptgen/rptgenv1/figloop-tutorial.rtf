function LocalMakePeaks(h)\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
pSize=49;\sa0\par\fi0\sb0
pData=peaks(pSize);\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
ax=axes('Parent',h,...\sa0\par\fi0\sb0
   'View',[-37.5 30],...\sa0\par\fi0\sb0
   'Visible','off',...\sa0\par\fi0\sb0
   'Xlim',[0 pSize],...\sa0\par\fi0\sb0
   'Ylim',[0 pSize],...\sa0\par\fi0\sb0
   'Zlim',[min(min(pData)) max(max(pData))],...\sa0\par\fi0\sb0
   'Units','normalized',...\sa0\par\fi0\sb0
   'Position',[0 0 1 1]);\sa0\par\fi0\sb0
\sa0\par\fi0\sb0
surface(pData,...\sa0\par\fi0\sb0
   'Parent',ax,...\sa0\par\fi0\sb0
   'FaceLighting','gouraud',...\sa0\par\fi0\sb0
   'edgecolor','none');\sa0\par\fi0\sb0
light('Parent',ax);\sa0\par\fi0\sb0
