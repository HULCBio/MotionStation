<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>13</matlabrelease>
<name>Simulink Report Generator</name>
<type>simulink</type>
<icon>$toolbox/matlab/icons/simulinkicon.gif</icon>
<help_location>$docroot/toolbox/rptgenext</help_location>

<list>

<listitem>
  <label>Report File List</label>
  <callback>rptlist</callback>
  <icon>$toolbox/rptgen/resources/Report.png</icon>
</listitem>

<listitem>
  <label>Report Explorer</label>
  <callback>setedit</callback>
  <icon>$toolbox/rptgen/resources/ReportGenerator.png</icon>
</listitem>

<listitem>
  <label>Source File Converter</label>
  <callback>rptconvert</callback>
  <icon>$toolbox/rptgen/resources/Convert.png</icon>
</listitem>

