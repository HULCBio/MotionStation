      MaskEnableString	      "on,off,on,on,on,on,on,off,on"
      MaskVisibilityString    "on,on,on,on,on,on,on,on,on"
      MaskToolTipString	      "on,on,on,on,on,on,on,on,on"
      MaskVarAliasString      ",,,,,,,,"
      MaskVariables	      "SampleRate=&1;UserRate=@2;SampleWidth=&3;Stereo"
