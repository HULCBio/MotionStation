      MaskIconFrame	      on
      MaskIconOpaque	      on
      MaskIconRotate	      "none"
      MaskIconUnits	      "autoscale"
      MaskValueString	      "8000|16000|16|off|512|3|on|INPUTDEV|Double"
    }
